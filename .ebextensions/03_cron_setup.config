files:
    "/etc/cron.d/mycron":
        mode: "000644"
        owner: root
        group: root
        content: |
            * * * * * root cd /var/app/current && /var/app/venv/staging-LQM1lest/bin/flask lookup-horse >> /var/log/wf-lookup.log > 2>&1

commands:
  remove_old_cron:
    command: "rm -f /etc/cron.d/mycron.bak"